{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./src/@types"
    ],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "src/*": [
        "src/*"
      ]
    },
  },
  "exclude": [
    "node_modules"
  ]
}