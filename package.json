{
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@supabase/supabase-js": "^2.23.0",
    "expo": "~48.0.15",
    "expo-secure-store": "~12.1.1",
    "expo-status-bar": "~1.4.4",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-table-component": "github:sturmenta/react-native-table-component#e1582aaba92b7e070c31c98b116eea9c23e17f6d",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^9.2.0",
    "zod": "^3.21.4",
    "@react-native-community/datetimepicker": "6.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@faker-js/faker": "^8.0.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.27",
    "@types/react-native-table-component": "^1.2.4",
    "@types/react-native-vector-icons": "^6.4.13",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-universe": "^11.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-commit-msg-linter": "^5.0.4",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "prettier": "^2.8.8",
    "react-native-dotenv": "^3.4.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  },
  "eslintConfig": {
    "extends": [
      "universe/native",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "rules": {
      "import/order": "off",
      "no-console": "error",
      "no-magic-numbers": "error",
      "no-unused-vars": "error",
      "semi": "error"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/src/**/*.d.ts",
      "!<rootDir>/src/tests/**/*",
      "<rootDir>/src/**/*.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "~(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "main": "node_modules/expo/AppEntry.js",
  "name": "meu-carango-rn",
  "private": true,
  "scripts": {
    "android": "expo start --android",
    "build:apk": "npx expo-doctor && eas build -p android --profile preview",
    "dev": "npm start --tunel",
    "ios": "expo start --ios",
    "lint": "npx eslint ./src/**",
    "lint:fix": "npx eslint ./src/** --fix",
    "start": "expo start",
    "test": "jest",
    "test:ci": "jest --ci --coverage --runInBand --verbose",
    "test:watch": "jest --watch"
  },
  "version": "1.0.0"
}
